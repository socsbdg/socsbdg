#include<stdio.h>

int main(){
    char x;
    int y;
    float frekuensi;

    scanf("%c%d", &x,&y);
    if(x=='C'){
        frekuensi=261.63;
    }
    else if(x=='D'){
        frekuensi=293.66;
    }
    else if(x=='E'){
        frekuensi=329.63;
    }    
    else if(x=='F'){
        frekuensi=349.23;
    }
    else if(x=='G'){
        frekuensi=392.00;
    }
    else if(x=='A'){
        frekuensi=440.00;
    }
    else if(x=='B'){
        frekuensi=493.88;
    }
    
    printf("Nada %c%d adalah %.2f Hz\n",x,y,frekuensi);
    return 0;
}
